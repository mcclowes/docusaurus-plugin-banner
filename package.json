{
  "name": "docusaurus-plugin-banner",
  "version": "0.1.0",
  "description": "A customizable dismissable banner plugin for Docusaurus sites with localStorage persistence.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@docusaurus/types": "^2.4.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/core": "^2.4.0",
    "@docusaurus/types": "^2.4.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "docusaurus",
    "plugin",
    "banner",
    "dismissable",
    "notification",
    "localstorage",
    "typescript"
  ]
}

